		{
			"name": "RAG Query Endpoints",
			"item": [
				{
					"name": "1. Query - Risk Analysis",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Status code is 200\", function () {",
									"    pm.response.to.have.status(200);",
									"});",
									"",
									"pm.test(\"Response has success field\", function () {",
									"    var jsonData = pm.response.json();",
									"    pm.expect(jsonData).to.have.property('success');",
									"    pm.expect(jsonData.success).to.eql(true);",
									"});",
									"",
									"pm.test(\"Response contains Groq analysis\", function () {",
									"    var jsonData = pm.response.json();",
									"    pm.expect(jsonData).to.have.property('analysis');",
									"    pm.expect(jsonData.analysis).to.have.property('summary');",
									"    pm.expect(jsonData.analysis).to.have.property('topRisks');",
									"    pm.expect(jsonData.analysis).to.have.property('teamAssessment');",
									"    pm.expect(jsonData.analysis).to.have.property('marketOutlook');",
									"});",
									"",
									"// Log the Groq response",
									"console.log('Groq Analysis Response:', JSON.stringify(pm.response.json().analysis, null, 2));"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"startupContext\": {\n    \"startupName\": \"TechFlow AI\",\n    \"location\": \"San Francisco, CA\",\n    \"stage\": \"Series A\",\n    \"sector\": \"SaaS\",\n    \"businessModel\": \"B2B\",\n    \"website\": \"https://techflow.ai\",\n    \"fundingRaised\": \"$2.5M\",\n    \"teamSize\": \"15\"\n  },\n  \"userQuery\": \"What are the main risks for this startup?\"\n}",
							"options": {
								"raw": {
									"language": "json"
								}
							}
						},
						"url": {
							"raw": "{{baseUrl}}/api/rag/query",
							"host": [
								"{{baseUrl}}"
							],
							"path": [
								"api",
								"rag",
								"query"
							]
						},
						"description": "Query the RAG system to get risk analysis. This endpoint retrieves relevant context from the vector store and uses Groq AI to generate insights."
					},
					"response": []
				},
