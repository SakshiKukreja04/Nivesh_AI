{
	"info": {
		"_postman_id": "nivesh-ai-api-collection",
		"name": "NiveshAI Backend API",
		"description": "Complete API collection for testing NiveshAI startup analysis endpoints",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
