	"item": [
		{
			"name": "File Upload & Analysis",
			"item": [
				{
					"name": "1. Upload Pitch Deck and Analyze",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Status code is 200\", function () {",
									"    pm.response.to.have.status(200);",
									"});",
									"",
									"pm.test(\"Response has success field\", function () {",
									"    var jsonData = pm.response.json();",
									"    pm.expect(jsonData).to.have.property('success');",
									"    pm.expect(jsonData.success).to.eql(true);",
									"});",
									"",
									"pm.test(\"Response contains analysis\", function () {",
									"    var jsonData = pm.response.json();",
									"    pm.expect(jsonData).to.have.property('analysis');",
									"    pm.expect(jsonData.analysis).to.have.property('summary');",
									"    pm.expect(jsonData.analysis).to.have.property('topRisks');",
									"    pm.expect(jsonData.analysis).to.have.property('teamAssessment');",
									"    pm.expect(jsonData.analysis).to.have.property('marketOutlook');",
									"});",
									"",
									"pm.test(\"Response contains startupId\", function () {",
									"    var jsonData = pm.response.json();",
									"    pm.expect(jsonData).to.have.property('startupId');",
									"});",
									"",
									"// Store startupId for later use",
									"if (pm.response.json().startupId) {",
									"    pm.environment.set('startupId', pm.response.json().startupId);",
									"}"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [],
						"body": {
							"mode": "formdata",
							"formdata": [
								{
									"key": "startupName",
									"value": "TechFlow AI",
									"type": "text"
								},
								{
									"key": "location",
									"value": "San Francisco, CA",
									"type": "text"
								},
								{
									"key": "stage",
									"value": "Series A",
									"type": "text"
								},
								{
									"key": "sector",
									"value": "SaaS",
									"type": "text"
								},
								{
									"key": "businessModel",
									"value": "B2B",
									"type": "text"
								},
								{
									"key": "website",
									"value": "https://techflow.ai",
									"type": "text"
								},
								{
									"key": "fundingRaised",
									"value": "$2.5M",
									"type": "text"
								},
								{
									"key": "teamSize",
									"value": "15",
									"type": "text"
								},
								{
									"key": "additionalNotes",
									"value": "A promising B2B SaaS startup with strong growth metrics and proven team.",
									"type": "text"
								},
								{
									"key": "pitchDeck",
									"type": "file",
									"src": [],
									"description": "Upload a PDF pitch deck or text file"
								}
							]
						},
						"url": {
							"raw": "{{baseUrl}}/api/analyze",
							"host": [
								"{{baseUrl}}"
							],
							"path": [
								"api",
								"analyze"
							]
						},
						"description": "Upload startup files (PDF pitch deck, transcript, or email) along with metadata to get AI-powered analysis. The files are processed, chunked, embedded, and stored in the vector database for RAG retrieval."
					},
					"response": []
